{
	"commandList": [
		{
			"type": "js",
			"regex": "js-example|javascript example",
			"path": "src/js-modules/example-js.js",
			"hidden": true
		}, {
			"type": "python",
			"regex": "py-example|python example",
			"path": "src/py-modules/example-python.py",
			"hidden": true
		}, {
